pipeline{
	agent any
    stages{
       stage("checkout"){
		steps{
	checkout scm
		}
	}
	stage("build"){
		steps{
			step{
	bat "javac Calculator.java"
	}
		}
	}
    stage("Success"){
	echo "Success"
	steps{
		bat "java Calculator"
	}
	}
}
}
post{
	always{
		archiveArtifacts artifacts: '*.class', fingerprint: true
	}
}